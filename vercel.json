{
 "version": 2,
 "builds": [
  {
   "src": "index.js",
   "use": "@vercel/node",
   "config": { "includeFiles": ["dist/**"] }
  }
 ],
 "routes": [
  {
   "src": "/(.*)",
   "dest": "index.js",
    "methods": ["GET", "POST", "DELETE", "PUT", "PATCH", "OPTIONS"],
    "headers": { "Access-Control-Allow-Origin": "https://vshop-backend-g1e4.vercel.app/" }
  }
 ]
}